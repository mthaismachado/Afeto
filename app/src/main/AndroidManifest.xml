<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team.afeto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.PerfilActivity" android:theme="@style/NoActionBar"></activity>
        <activity
            android:name=".Activity.LoginActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.SplashActivity"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Dashboard_area_logada"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.Dashboard_preciso_ajuda"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.SingUpActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.CadastroActivity"
            android:theme="@style/NoActionBar" />
    </application>

</manifest>